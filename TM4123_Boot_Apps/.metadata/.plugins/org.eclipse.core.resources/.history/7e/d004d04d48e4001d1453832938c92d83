#include "UART_Interface.h"
#include "timercnter_interface.h"
#include "DS3231_Interface.h"
#include "SSD1306_Interface.h"

void delay(uint8_t count);

void main()
{

    unsigned int *apps_stack_ptr;

    int a = 0 , b = 0;

    apps_stack_ptr  = 0x20004000 + (4*1024);

    DigitalWrite(PF2, HIGH);
    for(a=0 ; a<10000 ; a++)
    {
        for(b = 0 ; b< 10000 ; b++);

    }
    DigitalWrite(PF2, LOW);

    void (*app_entry_point)(void);
    app_entry_point = (void(*)(void)) (*((unsigned int*)(0x00020000 + 4)));

    if((*((unsigned int*)(0x00020000 + 4))) != 0xffffffff)
    {
        __set_MSP(apps_stack_ptr);
        app_entry_point();
    }
    else
    {
        DigitalWrite(PF2,HIGH);
    }

    a = 5;
}


